<Window x:Class="ManuelitoWpf.Classifica"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ManuelitoWpf"
      mc:Ignorable="d" 
      d:DesignHeight="730" d:DesignWidth="350"
      Title="Classifica">
    <Grid>
        <Grid.Background>
            <ImageBrush ImageSource="/images/menu.png" Stretch="UniformToFill"></ImageBrush>
        </Grid.Background>
        <ListBox x:Name="lst_classifica" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,159,0,0"></ListBox>
    </Grid>
</Window>
